{
    "name": "promptcraft-backend",
    "version": "1.0.0",
    "description": "PromptCraft backend - AI prompt evaluation platform",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "lint": "eslint src --ext .ts",
        "type-check": "tsc --noEmit",
        "test": "jest --runInBand",
        "test:coverage": "jest --coverage",
        "db:migrate": "node --require ts-node/register node_modules/.bin/sequelize-cli db:migrate",
        "db:migrate:undo": "node --require ts-node/register node_modules/.bin/sequelize-cli db:migrate:undo",
        "db:seed": "node --require ts-node/register node_modules/.bin/sequelize-cli db:seed:all",
        "db:seed:undo": "node --require ts-node/register node_modules/.bin/sequelize-cli db:seed:undo:all",
        "db:reset": "node --require ts-node/register node_modules/.bin/sequelize-cli db:migrate:undo:all && node --require ts-node/register node_modules/.bin/sequelize-cli db:migrate && node --require ts-node/register node_modules/.bin/sequelize-cli db:seed:all"
    },
    "keywords": [
        "promptcraft",
        "ai",
        "prompt-engineering",
        "evaluation"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^7.2.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "pg": "^8.11.5",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.3",
        "uuid": "^9.0.1",
        "winston": "^3.13.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.12.12",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "sequelize-cli": "^6.6.2",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.3",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5"
    }
}